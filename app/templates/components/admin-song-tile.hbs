<div class="admin-song-tile">
  <h3>{{song.title}}</h3>
  <p>{{song.length}}</p>
  <p>credits: {{song.writer}}</p>
  <audio controls src={{song.recording}}></audio><br/>
  <a href={{song.videoLink}} target="blank">Video Link</a>
  <p>{{song.lyrics}}</p>
  {{admin-edit-song song=song editSong="editSong" deleteSong="deleteSong"}}
</div>
